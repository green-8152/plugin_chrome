{
    "name": "Chrome Sniffer",
    "description": "Plugin Sniffer",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16":"icons/sniffer16.png",
        "32":"icons/sniffer32.png",
        "48":"icons/sniffer48.png",
        "128":"icons/sniffer128.png"
    },
    "action": {
        "default_popup":"popup.html"
    },
    "permissions": ["webRequest"],
    "host_permissions": ["https://*/*"],
    "background":{
        "service_worker": "background.js",
        "type": "module"
    }
}
